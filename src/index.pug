extends layout/layout
block content
  -
    var pages = [
      {name: 'Home', path:{en:'home-en.html', ar:'home-ar.html'}}, 
      {name: 'term and condition', path:{en:'termandcondition-en.html', ar:'termandcondition-ar.html'}}, 
      {name: 'cluster overview', path:{en:'cluster-overview-en.html', ar:'cluster-overview-ar.html'}}, 
      {name: 'Cluster Detail', path:{en:'cluster-detail-en.html', ar:'cluster-detail-ar.html'}},
      {name: 'Press Release', path:{en:'press-release-en.html', ar:'press-release-ar.html'}},
      {name: 'Free Zone', path:{en:'free-zone-en.html', ar:'free-zone-ar.html'}},
      {name: 'Strategic Location', path:{en:'strategic-location-en.html', ar:'strategic-location-ar.html'}},
      {name: 'Warehouse', path:{en:'warehouse-en.html', ar:'warehouse-ar.html'}},
      {name: 'Abu Dhabi', path:{en:'abu-dhabi-en.html', ar:'abu-dhabi-ar.html'}},
      {name: 'Investor Benefits', path:{en:'investor-benefits-en.html', ar:'investor-benefits-ar.html'}},
      {name: 'Campaign-Without-Nav', path:{en:'campaign-without-nav.html', ar:'campaign-without-nav.html'}},
      {name: '404', path:{en:'404-en.html', ar:'404-ar.html'}},
      {name: 'Contact', path:{en:'contact-en.html', ar:'contact-ar.html'}},

    //- {name: 'Contact Us', path:{en:'contact-en.html', ar:'contact-ar.html'}},
      //- {name: 'Component', path:{en:'component-en.html'}}
    ]
  
  div.placeholder
    div.container.py-5.d-flex
      ul.w-100.p-4
        each page in pages
          li.d-flex.justify-content-between.p-3.border-bottom
            span= page.name
            div
              a.px-5(href=page.path.en) English
              a(href=page.path.ar) Arabic

